{
  "window.titleBarStyle": "native",
  "window.menuBarVisibility": "toggle",
  "window.title": "${dirty}${activeEditorLong}",
  "editor.hover.delay": 600,
  "editor.multiCursorModifier": "ctrlCmd",
  "git.confirmSync": false,
  "editor.minimap.renderCharacters": false,
  "git.autofetch": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "javascript.preferGoToSourceDefinition": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.maxTokenizationLineLength": 2500,
    "editor.codeActionsOnSave": {
      "source.addMissingImports.ts": "always",
      "source.fixAll.eslint": "always",
      "source.fixAll.ts": "always",
      "source.organizeImports": "always",
      "source.removeUnused.ts": "always",
      "source.removeUnusedImports": "always",
      "source.sortImports": "always"
    },
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "on",
      "strings": "on"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.scrollBeyondLastLine": true,
  "editor.snippets.codeActions.enabled": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.formatOnSave": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "files.autoSaveWhenNoErrors": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "none",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.enableSmartCommit": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "recentlyUsedByPrefix"
  },
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "editor.minimap.enabled": false,
  "editor.cursorWidth": 0,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dart.enableCompletionCommitCharacters": true,
  "dart.previewCommitCharacters": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.sideBar.location": "right",
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": false,

  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.enablePreview": false,
  "window.newWindowDimensions": "inherit",
  "files.trimTrailingWhitespace": true,
  "diffEditor.renderSideBySide": false,
  "editor.snippetSuggestions": "bottom",
  "editor.detectIndentation": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "workbench.editor.showTabs": "none",
  "editor.formatOnSaveMode": "file",
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "editor.guides.indentation": false,
  "editor.fontFamily": "Fira Code",
  "editor.suggestFontSize": 16,
  "editor.suggestLineHeight": 30,
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.fontSize": 12,
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'calt', 'dlig'",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "keyword",
          "variable.language",
          "entity.other.attribute-name.html",
          "entity.other.attribute-name",
          "keyword.control",
          "storage.type",
          "comment",
          "comment.block",
          "comment.line"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.enabled": false,
  "zenMode.centerLayout": false,
  "zenMode.showTabs": "none",
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.scrollbar.verticalScrollbarSize": 1,
  "workbench.activityBar.location": "hidden",
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnType": true,
    "editor.wordBasedSuggestions": "allDocuments",
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.autoSave": "afterDelay"
  },
  "workbench.statusBar.visible": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "breadcrumbs.enabled": false,
  "emojisense.languages": {
    "markdown": true,
    "plaintext": {
      "markupCompletionsEnabled": false,
      "emojiDecoratorsEnabled": false
    },
    "scminput": true,
    "git-commit": true,
    "dart": true
  },
  "workbench.editor.empty.hint": "hidden",
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "[css]": {
    "aeditor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSave": "afterDelay",
  "makefile.configureOnOpen": true,
  "rust-analyzer.inlayHints.genericParameterHints.type.enable": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
  "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
  "cmake.showOptionsMovedNotification": false,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "pylint.path": ["/home/shivang/code/python/.venv/bin/pylint"],
  "database-client.autoSync": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.lineNumbers": "relative",
  "workbench.colorTheme": "Catppuccin Mocha",

  "workbench.iconTheme": "eq-material-theme-icons"
}
